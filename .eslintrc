{
  "parser": "@typescript-eslint/parser",
  "extends": ["plugin:@typescript-eslint/recommended"],
  "rules": {
      // "@typescript-eslint/no-inferrable-types": "off", 
      "prefer-const": 0,
      "semi": [2, "always"],
      // "no-console":"warn",
      "quotes": [
          2,
          "single",
          { "avoidEscape": true, "allowTemplateLiterals": true }
      ]
  },
  "parserOptions": {
      "requireConfigFile": false
  }
}
